{
    "docker_info": {
        "base_image": "ubuntu:16.04",
        "multi_stage_build": false,
        "exposed_ports": [],
        "user": [],
        "labels": [],
        "health_checks": [],
        "build_args": [],
        "envs": [
            "ENV DEBIAN_FRONTEND noninteractive"
        ],
        "copy_instructions": [
            "WORKDIR /app/linkedgeodata",
            "COPY configuration.txt.dist .",
            "COPY start.sh .",
            "COPY wait-for-postgres.sh ."
        ],
        "execution": [
            {
                "command": "entrypoint",
                "arguments": [
                    "[",
                    "\"./wait-for-postgres.sh\",",
                    "\"./start.sh\"",
                    "]"
                ]
            }
        ],
        "run": [
            [
                "apt-get -y update --fix-missing && apt-get install -y postgresql-9.5-postgis-2.2 wget sudo curl osmosis gettext-base osmctools",
                ""
            ],
            [
                "wget -qO - http://cstadler.aksw.org/repos/apt/conf/packages.precise.gpg.key  | sudo apt-key add - && \\",
                "echo 'deb http://cstadler.aksw.org/repos/apt precise main contrib non-free' | sudo tee -a /etc/apt/sources.list.d/cstadler.aksw.org.list &&",
                "apt update &&",
                "apt install -y linkedgeodata"
            ],
            [
                "chmod +x ./start.sh ./wait-for-postgres.sh",
                "RUN sleep 3"
            ]
        ]
    }
}